<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="property" default-lazy="true" package="com.jimueshop.domain">
	<class name = "Admin" table="TAB_ADMIN">
		<id name = "id" column="ADMIN_ID">
			<generator class="native" />
		</id>
		<property name="name" column = "ADMIN_NAME" /> 
		<property name="password" column = "ADMIN_PASSWORD" />
	</class>
</hibernate-mapping>