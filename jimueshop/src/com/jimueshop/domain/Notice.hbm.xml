<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="property" default-lazy="true" package="com.jimueshop.domain">
	<class name = "Notice" table="TAB_NOTICE">
		<id name = "id" column="NOTICE_ID">
			<generator class="native" />
		</id>
		<property name="noticeTitle" column = "NOTICE_TITLE" />
		<property name="noticeContent" column = "NOTICE_CONTENT" />
		<property name="noticeTime" column = "NOTICE_TIME" />
	</class>
</hibernate-mapping>